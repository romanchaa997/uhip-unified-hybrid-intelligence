# UHIP Example Configuration File
# Copy this to .env and customize for your environment

# Parallel Processing Configuration
UHIP_MAX_WORKERS=4
UHIP_USE_PROCESSES=false
UHIP_BATCH_SIZE=32
UHIP_TIMEOUT=300

# Optimization Configuration
UHIP_AUTO_OPTIMIZE=true
UHIP_OPT_INTERVAL=100

# Logging Configuration
UHIP_LOG_LEVEL=INFO

# Module Configuration (true/false)
UHIP_ENABLE_AI_ML=true
UHIP_ENABLE_QUANTUM=true
UHIP_ENABLE_BLOCKCHAIN=true
UHIP_ENABLE_EDGE=true

# Performance Configuration
UHIP_CACHE_ENABLED=true
UHIP_CACHE_SIZE=1000
